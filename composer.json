{
  "name": "spryker-shop/quote-request-agent-page",
  "type": "library",
  "description": "QuoteRequestAgentPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/quote-request-agent-page-extension": "^1.0.0",
    "spryker-shop/shop-application": "^1.2.0",
    "spryker-shop/shop-ui": "^1.28.1",
    "spryker/application": "^3.8.0",
    "spryker/cart": "^5.9.0 || ^7.0.0",
    "spryker/company-user": "^2.10.0",
    "spryker/customer": "^7.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/price": "^5.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/quote-request": "^2.0.0",
    "spryker/quote-request-agent": "^2.0.0",
    "spryker/store": "^1.8.0",
    "spryker/symfony": "^3.1.0",
    "spryker/util-date-time": "^1.0.0"
  },
  "require-dev": {
    "spryker-shop/agent-page": "*",
    "spryker-shop/cart-page": "*",
    "spryker-shop/configurable-bundle-widget": "*",
    "spryker-shop/quote-request-page": "*",
    "spryker/code-sniffer": "*",
    "spryker/router": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker-shop/agent-page": "If you want to use agent layout templates: ^1.2.0",
    "spryker-shop/cart-page": "If you want to use cart page molecules: ^3.2.0",
    "spryker-shop/configurable-bundle-widget": "If you want to use configurable bundle widget functionality",
    "spryker-shop/company-user-agent-widget": "If you want to use company user auto-complete form: ^1.0.0",
    "spryker-shop/quote-request-page": "If you want to use quote request page molecules: ^2.0.0",
    "spryker/router": "Use this module if you want use the RouterProviderPlugin.",
    "spryker/silex": "Use this module when using plugins that need Silex dependencies."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
