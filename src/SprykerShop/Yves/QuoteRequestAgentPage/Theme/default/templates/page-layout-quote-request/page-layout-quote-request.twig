{% extends template('page-layout-quote-request', 'QuoteRequestPage') %}

{% block breadcrumbs %}
    {% set breadcrumbSteps = [
        {
            label: 'agent.account.page_title' | trans,
            url: path('agent/overview'),
        },
        {
            label: 'quote_request_page.quote_request' | trans,
            url: path('agent/quote-request'),
        },
    ] %}
    {% set breadcrumbSteps = breadcrumbSteps | merge(breadcrumbExtension) %}

    {% include molecule('breadcrumb') with {
        data: {
            steps: breadcrumbSteps,
        },
    } only %}
{% endblock %}
